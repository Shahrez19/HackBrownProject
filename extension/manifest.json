{
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "Settings for Calmyo!"
    },
    "content_scripts": [{
        "js": ["background.js"],
        "matches": ["<all_urls>"]
    }],
    "permissions": [
        "activeTab"
    ],
    "description": "Preventing anxiety as it happens",
    "name": "CalmYo",
    "version": "1",
    "web_accessible_resources": ["embed.htm", "iframeInjector.js"]
}